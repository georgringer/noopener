{
    "name": "georgringer/noopener",
    "type": "typo3-cms-extension",
    "description": "Add rel=\"noopener noreferrer\" to all external links",
    "keywords": [
        "TYPO3",
        "extension",
        "security",
        "noopener",
        "noreferrer"
    ],
    "homepage": "https://ringer.it",
    "authors": [
        {
            "name": "Georg Ringer",
            "role": "Developer"
        }
    ],
    "license": [
        "GPL-2.0-or-later"
    ],
    "require": {
        "typo3/cms-core": "^8.7.0 || ^9.5.0"
    },
    "autoload": {
        "psr-4": {
            "GeorgRinger\\Noopener\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GeorgRinger\\Noopener\\Tests\\": "Tests"
        }
    },
    "replace": {
        "noopener": "self.version",
        "typo3-ter/noopener": "self.version"
    }
}
